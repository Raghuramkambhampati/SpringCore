<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
  <bean id="empsetter" class="org.raghu.springcore.beanclasses.Employee">
  <property name="id" value="1" />
  <property name="name" value="raghu" />
  <property name="age" value="25" />
  <property name="address" value="newjersey" />
  </bean>
  <bean id="empconst" class="org.raghu.springcore.beanclasses.EmployeeConst">
  <constructor-arg index="0" value="2"/>
  <constructor-arg index="1" value="ram"/>
  <constructor-arg index="2" value="30"/>
  <constructor-arg index="3" value="houston"/>
  </bean>
  <bean id="stringlist" class="org.raghu.springcore.beanclasses.StringList">
  <property name="l1">
  <list>
  <value>"Texas"</value>
  <value>"NewJersey"</value>
  <value>"California"</value>
  <value>"Ohio"</value>
  </list>
  </property>
   <property name="l2">
  <list>
  <value>11</value>
  <value>35</value>
  <value>47</value>
  <value>89</value>
  </list>
  </property>
  </bean>
  </beans>
  